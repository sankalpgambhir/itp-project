DECLARE PLUGIN "colog.plugin"


{

open Ltac_plugin
(*open Pp*)
open Stdarg

}

TACTIC EXTEND DoNothing
| [ "dno" int(i) ] ->
  {
    Ce_api.do_nothing i
  }
END

TACTIC EXTEND ChcAuto
| [ "chc_auto" ] ->
  {
    Ce_api.chc_auto ()
  }
| [ "chc_auto" "with" "depth" int(d) ] ->
  {
    Ce_api.chc_auto ~max_depth:d ()
  }
END
